{
  "name": "stiloso",
  "version": "0.5.0",
  "description": "Create styled and classed react components",
  "repository": "parro-it/stiloso",
  "author": "Andrea <andrea@parro.it>",
  "license": "MIT",
  "files": [
    "index.js",
    "example.js",
    "main.js"
  ],
  "scripts": {
    "test": "rollup -c && ava && xo --ignore index.js"
  },
  "keywords": [],
  "tonicExampleFilename": "example.js",
  "module": "main.js",
  "devDependencies": {
    "ava": "^0.19.1",
    "documentation": "^4.0.0-rc.1",
    "eslint-config-xo-preact": "^1.0.0",
    "eslint-plugin-react": "^7.0.0",
    "preact": "^8.1.0",
    "preact-render-to-string": "^3.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "xo": "^0.18.1",
    "flatmap": "^0.0.3",
    "html-tags": "^1.2.0"
  },
  "xo": {
    "extends": "xo-preact",
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-filename-extension": 0,
      "import/extensions": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0
    },
    "envs": [
      "browser",
      "node"
    ]
  },
  "peerDependencies": {
    "preact": "^8.1.0"
  }
}
