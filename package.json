{
  "name": "stiloso",
  "version": "0.0.1",
  "description": "Create styled and classed react components",
  "repository": "parro-it/stiloso",
  "author": "Andrea <andrea@parro.it>",
  "license": "MIT",
  "files": [
    "index.js",
    "example.js",
    "main.js"
  ],
  "scripts": {
    "test": "rollup -c && ava && xo --ignore example.js --ignore index.js"
  },
  "keywords": [],
  "tonicExampleFilename": "example.js",
  "module": "main.js",
  "devDependencies": {
    "ava": "^0.19.1",
    "documentation": "^4.0.0-rc.1",
    "eslint-config-xo-preact": "^1.0.0",
    "eslint-plugin-react": "^6.10.3",
    "preact-render-to-string": "^3.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-jsx": "^1.0.3",
    "xo": "^0.18.1"
  },
  "dependencies": {
    "preact": "^8.1.0"
  },
  "xo": {
    "extends": "xo-preact",
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-filename-extension": 0
    },
    "envs": [
      "browser",
      "node"
    ]
  }
}
